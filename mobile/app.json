{
  "expo": {
    "name": "AttendIT",
    "description": "Delegating attendance to the attendees",
    "slug": "AttendIT",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "assets/images/favicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "plugins": [
      "expo-router",
      "expo-secure-store",
      ["expo-build-properties", {
        "android": {
          "usesCleartextTraffic": true
        }
      }]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "10423375-1889-4c32-accc-fdaa94b1c423"
      }
    },
    "android": {
      "package": "com.solvedbiscuit71.AttendIT"
    }
  }
}
